$germany06_name_text = "Borzasztó zûrzavar";
$germany06_location_text = "Köln 1984-1986";
$germany06_animation_text = "Kölnben a tömegközlekedésért felelõs vezetõnek szörnyen nagy szüksége van segítségre.\nA város káoszban van, köszönhetõen a rosszul üzemeltetett tömegközlekedési rendszerének és te vagy az egyetlen, aki segíteni tud.";
$germany06_briefing_short = "Kölnben a tömegközlekedésért felelõs vezetõnek szörnyen nagy szüksége van segítségre.\nA város káoszban van, köszönhetõen a rosszul üzemeltetett tömegközlekedési rendszerének és te vagy az egyetlen, aki segíteni tud.";
$germany06_briefing_text = "Psszt, hé te ott! Igen, én vagyok az aki hívta önt... Én vagyok a tömegközlekedési osztály elnöke. Shh.. csak halkan, oké? Segítségre van szükségem a tömegközlekedési rendszer megjavításában. Úgy gondolja, hogy képes rá?";
$germany06_completed_text = "Köszönöm, tartozok önnek. Megmentette a bõrömet és segített az egész városnak! Teljes káoszban lennénk, ha ön nem lett volna itt. Ha bármikor szüksége van egy szívességre, tudja hogy hova kell jönnie.";

$germany06_task01_name = "Több és több villamost";
$germany06_task01_text = "Rendben, a következõt javaslom: elõször is, adjon a rendszerhez több villamost. Nagyon népszerûnek kellene lenniük. Ön választhatja a helyeket, biztos vagyok benne, hogy képes lesz kezelni!";
$germany06_task01_short = "Építs három villamosvonalat.";
$germany06_task01_passed = "Gyorsan, létesítsünk még pár viszonylatot!";

$germany06_task02_name = "A következõ, egy metróvonal";
$germany06_task02_text = "A következõ lépésünknek egy metróvonal építésének kellene lennie. Eléggé drága – csak vegyen fel kölcsönt, ha kell. Nekem kell ez a vonal!";
$germany06_task02_short = "Építs egy metróvonalat.";
$germany06_task02_passed = "Épp jókor, reménykedjünk, hogy ez pénzcsináló gép lesz!";

$germany06_task03_name = "Fizikai dolgozók, ugorjatok fel!";
$germany06_task03_text = "Próbálom rávenni a barátaimat, hogy váltsanak át tömegközlekedésre, de valami oknál fogva vonakodnak megtenni. Biztos vagyok benne, hogy több száz potenciálus utas van a fizikai dolgozók körében, tehát miért nem próbálja meg megnyerni õket?";
$germany06_task03_short = "Érj el %0%%-os hírnevet a fizikai dolgozók körében.";
$germany06_task03_passed = "Köszönöm! Most már busszal járhatok munkába a barátaimmal!";

$germany06_task04_name = "A hirdetés mûködik!";
$germany06_task04_text = "Mit szólna mondjuk egy kis hirdetéshez? Gyorsan sok új utasra van szükségünk!";
$germany06_task04_short = "Indíts két hirdetõkampányt.";
$germany06_task04_passed = "Úgy tûnik, hogy mûködik!";

$germany06_task05_name = "Rossz kezelés!";
$germany06_task05_text = "Jobban kellene bánnia velünk, idõsebb polgárokkal! Van vásárlóerõnk és nagyon hûségesek vagyunk. Úgy gondolom többet kellene tennie értünk!";
$germany06_task05_short = "Érj el %0%%-os hírnevet a nyugdíjasok között.";
$germany06_task05_passed = "Talán többet is tehetne, de jelenleg ez megteszi.";

$germany06_task06_name = "Mit szól a buszokhoz?";
$germany06_task06_text = "Mit szól pár buszhoz következõnek? Bármi ami mozgásban tart minket!";
$germany06_task06_short = "Létesíts három buszjáratot.";
$germany06_task06_passed = "Kicsit rövidek, de talán sok pénzt hoznak, lássuk.";

$germany06_task07_name = "Gyorsan, egy helikopter!";
$germany06_task07_text = "Kedvesem... Szörnyen nagy szükségem van egy helikopterjáratora! Azonnal! Hogyan várhatják el, hogy jelenjek meg a legújabb divatban egy szép helikopter nélkül, ami mellett pózolhatok! Lenne kedves és kisegítene?";
$germany06_task07_short = "Létesíts egy helikopterjáratot.";
$germany06_task07_passed = "Köszönöm, kedves! A fotózás pompás volt, a helikopterleszálló elõtti sorokról ítélve, nem fog hiányt szenvedni utasokból a jövõben!";

$germany06_task08_name = "A reptérre";
$germany06_task08_text = "A reptér nagyon nyüzsgõ helynek tûnik, talán odaterelhetnénk pár utast... Létesítene pár viszonylatot?";
$germany06_task08_short = "Létesíts három viszonylatot a {0:reptérhez}.";
$germany06_task08_passed = "Jól néz ki – gyorsan pénzt kell hoznia!";

$germany06_task09_name = "Nagyobb sebességet a metrónak";
$germany06_task09_text = "Van egy új találmányom – segít a metrószerelvényeknek egy kicsit nagyobb sebességet elérni kis távolságokon. Ha biztosítani tudná a finanszírozást szívesen felszerelném a metrószerelvényeire!";
$germany06_task09_short = "Fizess %it d,20 pénzt, hogy +%1%%-os sebességnövekedést érj el az összes metrószerelvényen négy hónapig.";
$germany06_task09_passed = "Elégedett vagyok, hogy a találmányom megfelelõen mûködik!";

$germany06_task10_name = "Még egy metróvonal";
$germany06_task10_text = "Még egy metróvonal kisegítene minket. Nagyon elégedett vagyok velük.";
$germany06_task10_short = "Építs egy metróvonalat.";
$germany06_task10_passed = "Remek, ennek meg kellene növelnie a bevételeinket.";

$germany06_task11_name = "Túl drága!";
$germany06_task11_text = "Kérem, segítsen ki minket! A kávézó a legjobb diákkedvezményekkel gyalog túl messze van az egyetemtõl. Nem tudunk odamenni ebédelni anélkül, hogy ne késnénk órákról. Ha létesítene egy összekötõ viszonylatot, biztos vagyok benne, hogy sok utasa lenne.";
$germany06_task11_short = "Létesíts egy buszjáratot a {0:kávézótól} az {1:egyetemig}.";
$germany06_task11_passed = "Köszönöm! Így már nem kell pénztelennek és éhesnek lennünk tanulás közben! Ön a mi hõsünk!";

$germany06_task12_name = "A villamosok kétszer olyan jók";
$germany06_task12_text = "Hozzáadhatnánk pár további villamost is – úgy tûnik, nagyon jól mûködnek a városban. Építsen nekünk pár vonalat!";
$germany06_task12_short = "Építs két villamosvonalat.";
$germany06_task12_passed = "Jól néz ki, szép munka a megállók elhelyezésével!";

$germany06_task13_name = "Zsúfolt kórház";
$germany06_task13_text = "A kórház parkolója kezd túlzsúfolt lenni! Tudna létesíteni pár tovább viszonylatot, hogy könnyebbé tegye a pácienseknek az elérésünket?";
$germany06_task13_short = "Létesíts két viszonylatot a {0:kórházhoz}.";
$germany06_task13_passed = "Köszönöm! A közlekedési dugók elmúltak, tehát a viszonylatai mûködnek!";

$germany06_task14_name = "Üzletemberek és a pénzük";
$germany06_task14_text = "Az üzletemberek szívesen költenek utazásra, tehát néhány szolgáltatásunkat nekik kell céloznunk. Biztosítaná, hogy tudják mit tudunk ajánlani nekik?";
$germany06_task14_short = "Érj el %0%%-os hírnevet az üzletemberek körében.";
$germany06_task14_passed = "Nagyon jó, kitûnõ munka!";

$germany06_task15_name = "Kedvesemi";
$germany06_task15_text = "Jó napot! A helyi iskolában tanítok és néhány tanulónknak gyalog kell hazamennie, mert a szüleiknek nincs ideje, hogy felvegyék õket. Annyira sajnálom ezeket a kicsiket, hogy gyalog kell hazajutniuk, tehát arra gondoltam, hogy talán ön megfontol néhány viszonylat létesítését, hogy tömegközlekedést használhassanak... akkor nem kellene kint lenniük az esõben!";
$germany06_task15_short = "Létesíts két viszonylatot az {0:iskolához}.";
$germany06_task15_passed = "Nagyon köszönöm! Így a gyerekek gyalog is mehetnek, vagy választhatják a tömegközlekedést is, ha hideg van vagy esik az esõ.";

$germany06_task16_name = "Több viszonylatot, gyorsan!";
$germany06_task16_text = "Több viszonylatra van szükségünk! Bármilyen régi dolog megteszi, csak bizonyosodjon meg róla, hogy pénzt hoz be.";
$germany06_task16_short = "Legyen %0 viszonylatod.";
$germany06_task16_passed = "Szép munk! Ön egy szélvész! Le vagyok nyûgözve a munkájától.";

$germany06_task17_name = "Díszesebb flotta";
$germany06_task17_text = "Talán több utast vonzanánk be, ha újabb jármûveink lennének? Az emberek imádják az új dolgokat – fizetnének, hogy kipróbálhassanak bármilyen új jármûvet, ami megveszünk. Modernizáljuk a flottát!";
$germany06_task17_short = "Ne legyen 18 hónapnál öregebb jármûved.";
$germany06_task17_passed = "Minden nagyon jól néz ki – ennek kényeztetnie kellene az utasokat és a sofõröket.";

$germany06_task18_name = "Egy rakat pénz";
$germany06_task18_text = "Ha egy kicsit több pénzt kereshetnénk, akkor azt hiszem át tudnám venni az irányítást. Nagy segítség volt!";
$germany06_task18_short = "Szerezz %it d,20 pénzt.";

$germany06_task19_name = "Múzeumlátogatók";
$germany06_task19_text = "A múzeum abban reménykedik, hogy több látogatót tud bevonzani. Segítene nekünk több viszonylat építésével, amelyek kijönnek ide? Nagyra méltányolnánk!";
$germany06_task19_short = "Létesíts három viszonylatot a {0:múzeumhoz}.";
$germany06_task19_passed = "Köszönet! A következõ kiállításunk a városi legendákról fog szólni, szakítson egy kis idõt rá és nézze meg!";

$germany06_task20_name = "Oktatási vészhelyzet!";
$germany06_task20_text = "Úgy tûnik, hogy egy kis problémám van. A kocsim lerobbant és most nagyon el vagyok foglalva az iskolában, ahol dolgozok. Tudom, hogy sokat kérek, de tudna létesíteni egy viszonylatot a házamtól az iskoláig, kérem?";
$germany06_task20_short = "Létesíts egy viszonylatot a {0:tanár házától} az {1:iskoláig}.";
